        "dev": "VITE_BUILD_DATE=$(date -Iseconds) VITE_BRANCH_ID=$BRANCH_ID VITE_BRANCH_NAME=$BRANCH_NAME VITE_BUILD_TAG=$BUILD_TAG vite --port 8100 --host 0.0.0.0",
